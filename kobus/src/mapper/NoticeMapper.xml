<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.NoticeMapper">

	<insert id="insertNotice" parameterType="NoticeModel">
		INSERT
		INTO notice
		VALUES (SEQ_KO_TER_TERCODE.nextVal, #{n_writer}, #{n_title}, #{n_content},
		sysdate, 0)
	</insert>
		
		<select id="selectAll"  resultType="NoticeModel">
		select
		n_id,
		n_writer,
		n_title,
		n_content,
		n_date,
		n_hit
		from notice
		<trim prefix="ORDER BY" >n_id desc</trim>
	</select>

</mapper>